{
    "commitListCommand": {
        "name": "reflog",
        "arguments": [ "show" ],
        "flags": [
            {
                "Name": "pretty",
                "Value": "--pretty=%h~%cn~%ch~%cI~%s"
            }
        ]
    },
    "fileListCommand": {
        "name": "ls-tree",
        "arguments": [
            "HEAD"
        ],
        "flags": [
            {
                "Name": "r",
                "Value": "-r"
            },
            {
                "Name": "name-only",
                "Value": "--name-only"
            }
        ]
    }
}